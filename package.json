  "scripts": {
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build && electron-builder --win --linux",
    "build:all": "npm run build",
    "preview": "electron-vite preview",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run typecheck && electron-vite build && electron-builder --win",
    "build:mac": "npm run typecheck && electron-vite build && electron-builder --mac",
    "build:linux": "npm run typecheck && electron-vite build && electron-builder --linux",
    "prepare": "husky"
  },
